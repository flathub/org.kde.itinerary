                "-DENABLE_LIBCURL=OFF",
                "-DENABLE_UTILS=OFF",
                "-DENABLE_QT5=OFF",
                "-DENABLE_QT6=OFF",
                "-DENABLE_SPLASH=OFF",
                "-DWITH_Cairo=OFF"
