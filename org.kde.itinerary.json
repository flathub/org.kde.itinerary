                "-DLIB_INSTALL_DIR=/app/lib",
                "-DBUILD_TRANSLATIONS=NO",
                "-DBUILD_WITH_QT6=ON"
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs"
            ],
            "modules": [
                {
                    "name": "libsecret",
                    "buildsystem": "meson",
                    "config-opts": [
                        "-Dmanpage=false",
                        "-Dvapi=false",
                        "-Dgtk_doc=false",
                        "-Dintrospection=false",
