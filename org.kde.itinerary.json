                    }
                }
            ],
            "modules": [
                {
                    "name": "openjpeg",
                    "buildsystem": "cmake-ninja",
                    "builddir": true,
                    "config-opts": [
                        "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                        "-DBUILD_STATIC_LIBS=OFF",
                        "-DBUILD_CODEC=OFF"
                    ],
                    "cleanup": [
                        "/bin",
                        "/include",
                        "/lib/*.a",
                        "/lib/openjpeg-*",
                        "/lib/pkgconfig"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://github.com/uclouvain/openjpeg/archive/v2.5.2.tar.gz",
